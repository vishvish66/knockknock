class computer:
    
    def __init__(self):
      self.name = "vish"
      self.age = 100
    
   

    def compare(self,other):
      if self.age == other.age:
        return True
      else:
        return False  

c1 = computer()
c1.age = 100
c2= computer()

if c1.compare(c2):
  print("same")
else:
  print("diff")  



print(c1.name)
print(c2.name)


#inharitance in python

class a:
  
  def __init__ (self):
    print("init of a")
  
  def feature1(self):
     print("fet 1 working")
  def feature2(self):
     print("fet 2 working")   

class b(a):
   
   def __init__ (self):
     print("init of b")
   
   
   def feature3(self):
     print("fet 3 working")
   def feature4(self):
     print("fet 4 working")   

class c(b):
   
   def __init__(self):
     super().__init__()
     print("in c int") 
   def feature5(self):
     print("fet 5 working")
   def feature6(self):
     print("fet 6 working")  

     
a1 = a()
a1.feature1()
a1.feature2()

b1 = b()
b1.feature3()
b1.feature4()

c1 = c()
c1.feature5()
c1.feature6()


class student:
     school = "vish"
     def __init__(self,m1,m2,m3):
       self.m1 = m1
       self.m2 = m2
       self.m3 = m3
     

     def avg(self):
          return(self.m1 + self.m2 + self.m3)/3

     @classmethod
     def getschoolname(cls):
         return cls.school    
     @staticmethod
     def info():
       print("this is vish class") 
       
s1=student(10,23,45)
s2=student(11,233,5)
s3=student(15,232,425)

print(s1.avg())
print(s2.avg())
print(s3.avg())

print(student.getschoolname())
